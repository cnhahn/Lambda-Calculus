
-------------------------------------------------------------------------------
-- DO NOT MODIFY THIS SEGMENT
-------------------------------------------------------------------------------

let TRUE  = \x y -> x
let FALSE = \x y -> y
let ITE   = \b x y -> b x y
let NOT   = \b x y -> b y x
let AND   = \b1 b2 -> ITE b1 b2 FALSE
let OR    = \b1 b2 -> ITE b1 TRUE b2

-------------------------------------------------------------------------------
-- YOU SHOULD ONLY MODIFY THE TEXT BELOW, JUST THE PARTS MARKED AS COMMENTS
-------------------------------------------------------------------------------

eval not_false :
  NOT FALSE
  -- (a) fill in your reductions here
  =d> (\b x y -> b y x) (\x y -> y)
  =a> (\b x y -> b y x) (\a b -> b)
  =b> (\x y -> (\a b -> b) y x) 
  =b> (\x y -> (\b -> b) x) 
  =b> (\x y -> x)
  =d> TRUE

eval and_false_true:
  AND FALSE TRUE
  -- (b) fill in your reductions here
  =d> (\b1 b2 -> ITE b1 b2 FALSE) (\x y -> y) (\x y -> x)
  =a> (\a b -> ITE a b FALSE) (\a b -> b) (\a b -> a)
  =b> (\b -> ITE (\a b -> b) b FALSE) (\a b -> a)

  =b> \x y -> y
  =d> FALSE

eval or_true_false :
  OR TRUE FALSE
  -- (c) fill in your reductions here
  =d> (\b1 b2 -> ITE b1 TRUE b2) (\x y -> x) (\x y -> y)
  =a> (\a b -> ITE a TRUE b) (\a b -> a) (\a b -> b)
  =b> (\b -> ITE (\a b -> a) TRUE b) (\a b -> b)

  =b> \x y -> x
  =d> TRUE
